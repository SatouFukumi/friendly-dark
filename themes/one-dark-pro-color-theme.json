				"foreground": "#C678DD"
			}
		},
		{
			"scope": "text.html.laravel-blade source.php.embedded.line.html support.constant.laravel-blade",
			"settings": {
				"foreground": "#C678DD"
			}
		},
		{
			"scope": "invalid.deprecated.entity.other.attribute-name.html",
