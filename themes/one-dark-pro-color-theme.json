	"semanticTokenColors": {
		"macro": {
			"foreground": "#b79ced",
			"fontStyle": "italic"
		},
		"variable": {
			"foreground": "#E5C07B",
			"fontStyle": ""
		},
		"parameter": {
			"foreground": "#ac92f3",
			"fontStyle": ""
		},
		"type": {
			"foreground": "#ee7ba7",
			"fontStyle": ""
		},
		"typeParameter": {
			"fontStyle": "",
			"foreground": "#ee7ba7"
		},
		"class": {
			"foreground": "#ee7ba7",
			"fontStyle": ""
		},
		"interface": {
			"foreground": "#ee7ba7",
			"fontStyle": ""
		},
		"templateType": {
			"foreground": "#ee7ba7",
			"fontStyle": ""
		},
		"operatorOverload": {
			"fontStyle": "",
			"foreground": "#61AFEF"
		},
		"memberOperatorOverload": {
			"fontStyle": "",
			"foreground": "#61AFEF"
		},
		"function": {
			"fontStyle": "",
			"foreground": "#61AFEF"
		},
		"method": {
			"fontStyle": "",
			"foreground": "#61AFEF"
		},
		"property": {
			"fontStyle": "",
			"foreground": "#E06C75"
		},
		"property.defaultLibrary": {
			"fontStyle": "",
			"foreground": "#E5C07B"
		},
		"namespace": {
			"foreground": "#ee7ba7",
			"fontStyle": ""
		},
		"enum": {
			"foreground": "#48cae4",
			"fontStyle": ""
		},
		"enumMember": {
			"foreground": "#48cae4",
			"fontStyle": "italic"
		}
	},
	"semanticHighlighting": true,
	"tokenColors": [
		// token: enum
		{
			"scope": [
				"entity.name.type.enum"
			],
			"settings": {
				"foreground": "#48cae4",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"variable.other.enummember"
			],
			"settings": {
				"foreground": "#48cae4",
				"fontStyle": "italic"
			}
		},
		// token: variable
		{
			"scope": [
				"variable.other.env",
				"support.constant.math",
				"variable.other.constant",
				"variable.other.readwrite",
				"variable.other.object",
				"support.type.object.module",
				"source.js support.type.object.module keyword.operator.accessor",
				"variable.language.this"
			],
			"settings": {
				"foreground": "#E5C07B",
				"fontStyle": ""
			},
			"name": "*"
		},
		{
			"scope": [
				"support.variable.magic.python"
			],
			"settings": {
				"foreground": "#E06C75",
				"fontStyle": ""
			},
			"name": "*"
		},
		// token: parameter
		{
			"scope": [
				"variable.parameter"
			],
			"settings": {
				"foreground": "#ac92f3",
				"fontStyle": ""
			}
		},
		// token: function/method
		{
			"scope": [
				"entity.name.function",
				"variable.function",
				"meta.require",
				"support.function.any-method",
				"variable.language.super"
			],
			"settings": {
				"foreground": "#61AFEF",
				"fontStyle": ""
			}
		},
		// token: property/import
		{
			"scope": [
				"variable.other.readwrite.alias",
				"meta.import variable.other.readwrite",
				"variable.object.property",
				"variable.other.property",
				"variable.other.constant.property",
				"variable.other.object.property",
				"support.variable.property"
			],
			"settings": {
				"foreground": "#E06C75",
				"fontStyle": ""
			}
		},
		// token: operator
		{
			"scope": [
				"keyword.operator.bitwise",
				"keyword.operator.assignment",
				"keyword.operator.logical",
				"keyword.operator.type",
				"keyword.operator.arithmetic",
				"keyword.operator.comparison",
				"keyword.operator.decrement",
				"keyword.operator.increment",
				"keyword.operator.relational"
			],
			"settings": {
				"foreground": "#56B6C2",
				"fontStyle": ""
			},
			"name": ""
		},
		// token: base_keyword/storage_type
		{
			"scope": [
				"keyword",
				"keyword.operator.expression",
				"keyword.operator.assignment.c",
				"keyword.operator.comparison.c",
				"keyword.operator.c",
				"keyword.operator.increment.c",
				"keyword.operator.decrement.c",
				"keyword.operator.bitwise.shift.c",
				"keyword.operator.assignment.cpp",
				"keyword.operator.comparison.cpp",
				"keyword.operator.cpp",
				"keyword.operator.increment.cpp",
				"keyword.operator.decrement.cpp",
				"keyword.operator.bitwise.shift.cpp",
				"storage.type",
				"storage.modifier",
				"punctuation.definition.template-expression"
			],
			"settings": {
				"foreground": "#d47bda",
				"fontStyle": ""
			}
		},
		// token: unit
		{
			"scope": [
				"keyword.other.unit",
			],
			"settings": {
				"foreground": "#E06C75",
				"fontStyle": "italic"
			}
		},
		// token: type/class
		{
			"scope": [
				"entity.name.type",
				"support.class, entity.name.type.class",
				"support.class",
				"entity.name.type.class",
				"entity.other.inherited-class",
				"support.class.component"
			],
			"settings": {
				"foreground": "#ee7ba7",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"support.type.primitive",
			],
			"settings": {
				"foreground": "#D19A66",
				"fontStyle": ""
			}
		},
		// token: constant(number, boolean, ...)
		{
			"scope": [
				"constant",
				"constant.numeric",
				"support.type.builtin"
			],
			"settings": {
				"foreground": "#D19A66",
				"fontStyle": ""
			}
		},
		// token: comment
		{
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#5C6370",
				"fontStyle": ""
			}
		},
		// token: punctuation 
		{
			"scope": [
				"keyword.operator.accessor",
				"keyword.operator.type.annotation",
				"keyword.operator.optional",
			],
			"settings": {
				"foreground": "#abb2bf",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"punctuation.accessor",
				"punctuation.separator"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#abb2bf"
			}
		},
		// token: json
		{
			"scope": "support.constant.json",
			"settings": {
				"foreground": "#D19A66"
			}
		},
		{
			"scope": "source.json meta.structure.dictionary.json > string.quoted.json",
			"settings": {
				"foreground": "#E06C75"
			}
		},
		{
			"scope": "source.json meta.structure.dictionary.json > string.quoted.json > punctuation.string",
			"settings": {
				"foreground": "#E06C75"
			}
		},
		{
			"scope": "source.json meta.structure.dictionary.json > value.json > string.quoted.json,source.json meta.structure.array.json > value.json > string.quoted.json,source.json meta.structure.dictionary.json > value.json > string.quoted.json > punctuation,source.json meta.structure.array.json > value.json > string.quoted.json > punctuation",
			"settings": {
				"foreground": "#98C379"
			}
		},
		{
			"scope": "source.json meta.structure.dictionary.json > constant.language.json,source.json meta.structure.array.json > constant.language.json",
			"settings": {
				"foreground": "#56B6C2"
			}
		},
		{
			"scope": "support.type.property-name.json",
			"settings": {
				"foreground": "#E06C75"
			}
		},
		{
			"scope": "support.type.property-name.json punctuation",
			"settings": {
				"foreground": "#E06C75"
			}
		},
		// token: html/css
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#E06C75"
			}
		},
		{
			"scope": "constant.character.entity",
			"settings": {
				"foreground": "#E06C75"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name",
				"entity.other.attribute-name.attribute",
			],
			"settings": {
				"foreground": "#cd5686"
			}
		},
		{
			"scope": [
				"meta.tag.attributes variable.other.readwrite.tsx"
			],
			"settings": {
				"foreground": "#cd5686"
			}
		},
		{
			"scope": "entity.other.attribute-name.id",
			"settings": {
				"foreground": "#61AFEF"
			}
		},
		{
			"scope": "entity.other.attribute-name.class.css",
			"settings": {
				"foreground": "#D19A66"
			}
		},
		{
			"scope": [
				"support.function"
			],
			"settings": {
				"foreground": "#56B6C2",
				"fontStyle": ""
			}
		},
		{
			"scope": "selector.sass",
			"settings": {
				"foreground": "#E06C75"
			}
		},
		{
			"scope": [
				"support.constant.property-value.css",
				"support.constant.property-value.scss",
				"meta.property-value support.type.property-name"
			],
			"settings": {
				"foreground": "#D19A66"
			}
		},
		{
			"scope": "keyword.operator.css,keyword.operator.scss,keyword.operator.less",
			"settings": {
				"foreground": "#56B6C2"
			}
		},
		{
			"scope": "support.constant.color.w3c-standard-color-name.css,support.constant.color.w3c-standard-color-name.scss",
			"settings": {
				"foreground": "#D19A66"
			}
		},
		{
			"scope": [
				"meta.attribute-selector"
			],
			"settings": {
				"foreground": "#98C379",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"keyword.operator.less"
			],
			"settings": {
				"foreground": "#D19A66",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.pseudo-class"
			],
			"settings": {
				"foreground": "#56B6C2",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"punctuation.separator.list.comma.css",
				"support.type.property-name",
				"source.css meta.attribute.style.html"
			],
			"settings": {
				"foreground": "#ABB2BF"
			}
		},
		{
			"scope": "invalid.illegal.bad-ampersand.html",
			"settings": {
				"foreground": "#ABB2BF"
			}
		},
		{
			"scope": "text.html.laravel-blade source.php.embedded.line.html entity.name.tag.laravel-blade",
